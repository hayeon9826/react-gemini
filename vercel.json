{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/index.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/server/index.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
